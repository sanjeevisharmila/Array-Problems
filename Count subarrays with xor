class Solution {
    public long subarrayXor(int arr[], int k) {
        int n=arr.length;
        int count=0;
        for(int i=0;i<n;i++)
        {
            int xor=0;
            for(int j=i;j<n;j++)
            {
                xor=xor^arr[j];
                    if(xor==k)
                    {
                        count++;
                    }
            }
        }
        return count;
    }
}
time=O(n*n)
space=O(1)
