
class Solution {
    static int inversionCount(int arr[]) {
        int count=0;
        for(int i=0;i<arr.length-1;i++)
        {
            for(int j=i+1;j<arr.length;j++)
            {
                if(arr[i]>arr[j])
                {
                    count++;
                }
            }
        }
        return count;
    }
}
time=O(n*n)
space=O(1)
